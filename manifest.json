{
  "manifest_version": 2,
  "name": "The Bill Wizard",
  "permissions": [
    "https://thebillwizard.com/"
  ],
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-1.12.4.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "page/popup.html"
  }
}

